#include <stdio.h>

int main(){
    int i, j;
    int A[3][3] = {{3,5,7},{1,4,9},{2,3,8}};
    int B[3][3] = {{1,2,3},{4,5,6},{7,8,9}};
    int C[3][3];
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            C[i][j] = A[i][j] + B[i][j];
        }
    }
    for(i=0;i<3;i++){
        printf("\n");
        for(j=0;j<3;j++){
            printf("%d ",C[i][j]);
        }
    }
}
